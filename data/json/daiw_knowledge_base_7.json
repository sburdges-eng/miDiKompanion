  "timing_feel_guide": {
    "ahead": {
      "description": "Notes land slightly before the beat",
      "emotional_quality": ["anxious", "urgent", "pushing", "aggressive"],
      "use_for": ["anxiety", "anger", "excitement", "impatience"]
    },
    "on": {
      "description": "Notes land precisely on the beat",
      "emotional_quality": ["present", "assertive", "steady", "grounded"],
      "use_for": ["confidence", "stability", "neutral", "marching"]
    },
    "behind": {
      "description": "Notes land slightly after the beat",
      "emotional_quality": ["relaxed", "heavy", "resigned", "laid back"],
      "use_for": ["grief", "calm", "nostalgia", "exhaustion", "blues"]
    }
  },

  "rule_breaking_categories": {
    "harmonic": {
      "parallel_fifths": {
        "classical_rule": "Forbidden - destroys voice independence",
