  "emotion_modifiers": {
    "ptsd_intrusion": {
      "name": "PTSD Intrusive Memories",
      "description": "Layer on base emotion for trauma response",
      "intrusion_probability": 0.15,
      "intrusion_types": [
        {
          "type": "register_spike",
          "description": "Sudden high notes, startling"
        },
        {
          "type": "harmonic_rush",
          "description": "Chords speed up briefly, panic"
        },
        {
          "type": "unresolved_dissonance",
          "description": "Dissonance that doesn't resolve correctly"
        },
        {
          "type": "dynamic_spike",
          "description": "Sudden loud moment"
        },
        {
          "type": "rhythmic_stumble",
          "description": "Timing disruption, tripping"
        }
      ]
    },

    "dissociation": {
      "name": "Dissociative Quality",
      "description": "Detached, floating, not quite present",
      "adjustments": {
        "dissonance": -0.1,
        "density": -0.3,
        "register_shift": 5
      },
      "timing_override": "behind",
      "notes": "Oddly smooth surface, disconnected from beat"
    },

    "misdirection": {
      "name": "Emotional Misdirection",
      "description": "Surface reads as one emotion, undertow is another",
      "adjustments": {
        "dissonance": 0.1
      },
      "techniques": [
        "Use major-leaning progressions",
        "Avoid perfect cadences",
        "Use inversions instead of root position",
        "Unresolved phrase endings",
        "Melody comfortable but harmony unsettled"
      ],
      "notes": "Key technique for Kelly song - sounds like love until reveal"
    },

    "suppressed": {
      "name": "Suppressed Emotion",
      "description": "Holding it together, tension underneath",
      "adjustments": {
        "density": -0.5,
        "dissonance": 0.15
      },
      "timing_override": "behind",
      "notes": "Dynamics stay controlled despite intense content"
    },

    "cathartic_release": {
      "name": "Cathartic Release",
      "description": "Finally letting go",
      "adjustments": {
        "tempo": 10,
        "dissonance": -0.2,
        "density": 0.5
      },
      "notes": "Dynamics expand, tension resolves, breathing again"
    }
  },

