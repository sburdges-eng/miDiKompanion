  "circumplex_model": {
    "description": "Two-dimensional model of emotion",
    "axes": {
      "valence": {
        "description": "Positive to negative emotional quality",
        "range": [-2, 2],
        "labels": {
          "-2": "very_negative",
          "-1": "negative", 
          "0": "neutral",
          "1": "positive",
          "2": "very_positive"
        }
      },
      "arousal": {
        "description": "Energy level / activation",
        "range": [-2, 2],
        "labels": {
          "-2": "very_low",
          "-1": "low",
          "0": "neutral", 
          "1": "high",
          "2": "very_high"
        }
      }
    },
    "quadrants": {
      "low_arousal_negative_valence": ["grief", "depression", "sadness", "melancholy"],
      "high_arousal_negative_valence": ["anxiety", "anger", "fear", "frustration"],
      "low_arousal_positive_valence": ["calm", "content", "peaceful", "serene"],
      "high_arousal_positive_valence": ["excited", "joyful", "elated", "energetic"]
    }
  },

