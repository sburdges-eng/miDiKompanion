						"media": {
							"image": "media/breakpoints.PNG",
							"altText": "Breakpoints"
						}
					},
					{
						"id": "startdebug",
						"title": "Start Debug Session",
						"description": "Click the button below, or the Play button in the top left corner of the debug panel, to start the debug session. If a list of configurations displays, choose your program name. \n[Run debug session](command:workbench.action.debug.start)",
						"media": {
							"image": "media/startdebug.PNG",
							"altText": "Start debug session"
						},
						"completionEvents": [
							"onCommand:workbench.action.debug.start"
						]
					},
					{
						"id": "cicsstart",
						"title": "Run Program",
						"description": "Run your program on your CICS region.",
						"media": {
							"image": "media/app-icon.png",
							"altText": "Empty image"
						}
					}
				]
			}
		],
