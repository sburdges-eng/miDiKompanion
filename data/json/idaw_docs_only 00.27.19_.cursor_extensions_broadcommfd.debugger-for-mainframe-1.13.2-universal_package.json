{
	"name": "debugger-for-mainframe",
	"displayName": "Debugger for Mainframe",
	"description": "Code4z debugging extension for CICS and Batch applications written in COBOL and High-Level Assembler Language",
	"version": "1.13.2",
	"author": {
		"name": "Broadcom"
	},
	"license": "Broadcom",
	"publisher": "BroadcomMFD",
	"engines": {
		"vscode": "^1.38.1"
	},
	"categories": [
		"Debuggers"
	],
	"keywords": [
		"intertest",
		"cics",
		"batch",
		"cobol",
		"assembler",
		"debug",
		"code4z",
		"zowe",
		"intertest-cics",
		"intertest-batch"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/BroadcomMFD/debugger-for-mainframe"
	},
	"bugs": {
		"url": "https://github.com/BroadcomMFD/debugger-for-mainframe/issues"
	},
	"activationEvents": [
		"onView:statementTrace",
		"onCommand:intertest-debug-dap.fetch-extended-source",
		"onCommand:intertest-debug-dap.list-composite",
		"onCommand:intertest-debug-dap.batch-convert-jcl",
		"onCommand:intertest-debug-dap.help-command",
		"onDebug"
	],
	"icon": "resources/logo.png",
	"main": "./out/main/extension",
	"contributes": {
		"configuration": [
			{
				"title": "Debugger for Mainframe",
				"properties": {
					"debuggerForMainframe.nativeCertificateStoreForWindows": {
						"type": "string",
						"enum": [
							"Java internal certificate",
							"Windows-ROOT"
						],
						"default": "Java internal certificate",
						"scope": "machine",
						"description": "Trust store path specific to Windows instead of using internal Java certificate store path",
						"order": 0
					},
					"debuggerForMainframe.nativeCertificateStoreForMacOS.trustStoreType": {
						"type": "string",
						"default": "Java internal certificate",
						"scope": "machine",
						"description": "Trust store type specific to Mac OS instead of using internal Java certificate store type",
						"order": 1
					},
					"debuggerForMainframe.nativeCertificateStoreForMacOS.trustStore": {
						"type": "string",
						"default": "Java internal certificate",
						"scope": "machine",
						"description": "Trust store path specific to Mac OS instead of using internal Java certificate store path",
						"order": 2
					}
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "statementTrace.prev",
					"when": "view == statementTrace",
					"group": "navigation@1"
				},
				{
					"command": "statementTrace.next",
					"when": "view == statementTrace",
					"group": "navigation@2"
				},
				{
					"command": "intertest-debug-dap.filter",
					"when": "!filtered && inDebugMode && (debugType == 'intertest-batch' || debugType== 'intertest-cics') && view == workbench.debug.variablesView",
					"group": "navigation@1"
				},
				{
					"command": "intertest-debug-dap.clearFilter",
					"when": "filtered && inDebugMode && (debugType == 'intertest-batch' || debugType== 'intertest-cics') && view == workbench.debug.variablesView",
					"group": "navigation@1"
				}
			],
			"view/item/context": [
				{
					"command": "statementTrace.copyStatemenTrace",
					"when": "view == statementTrace",
					"group": "navigation@1"
				}
			],
			"editor/context": [
				{
					"command": "intertest-debug-dap.setVariable",
					"when": "inDebugMode && (debugType == 'intertest-batch' || debugType== 'intertest-cics')",
					"group": "debug"
				},
				{
					"command": "intertest-debug-dap.gotoVariable",
					"when": "inDebugMode && (debugType == 'intertest-batch' || debugType== 'intertest-cics')",
					"group": "debug"
				}
			],
			"debug/variables/context": [
				{
					"command": "intertest-debug-dap.sort-definiton",
					"when": "inDebugMode && isAlphabetic && (debugType == 'intertest-batch' || debugType== 'intertest-cics')"
				},
				{
					"command": "intertest-debug-dap.sort-ascending",
					"when": "inDebugMode && !isAlphabetic  && (debugType == 'intertest-batch' || debugType== 'intertest-cics')"
				}
			]
		},
		"views": {
			"debug": [
				{
					"id": "statementTrace",
					"name": "Statement Trace"
				}
			]
		},
		"walkthroughs": [
			{
				"id": "batchDemo",
				"title": "Basic Batch Demo Session",
				"description": "This walkthrough takes you step-by-step through a basic batch demo debugging session in Debugger for Mainframe.",
				"steps": [
					{
						"id": "openlaunch",
						"title": "Open Debug Configuration",
						"description": "This step opens the launch.json file in your workspace. You use launch.json to configure your debugging session. \nIf no launch.json file exists in your workspace, a prompt will display at the top of the screen. Select Debugger for Mainframe: INTERTEST™ FOR BATCH - demo session from the list of possible configurations. \nIf you already have a launch.json file with existing debug configurations, click the Add Configuration... button in the bottom right corner to add a new one. \n[Open launch.json file](command:workbench.action.debug.configure)",
						"media": {
							"image": "media/app-icon.png",
							"altText": "Default image"
						},
						"completionEvents": [
							"onCommand:workbench.action.debug.configure"
						]
					},
					{
						"id": "populatelaunch",
						"title": "Populate Debug Configuration",
						"description": "Populate all fields in the launch.json file. \nConsult the \"Debugging Demo Sessions\" page of the Code4z documentation for parameter descriptions. \n[Documentation (opens in new window)](https://techdocs.broadcom.com/code4z)",
						"media": {
							"image": "media/b3-launch.png",
							"altText": "launch.json file with originalJCL element added"
						},
						"completionEvents": []
					},
					{
						"id": "convert",
						"title": "Convert JCL",
						"description": "Use the button below to convert your original JCL to the member you specified in the convertedJCL field. Enter your password if prompted. \n[Convert JCL](command:intertest-debug-dap.batch-convert-jcl)",
						"media": {
							"image": "media/b4-convert.png",
							"altText": "Convert JCL command in the F1 menu"
						},
						"completionEvents": [
							"onCommand:intertest-debug-dap.batch-convert-jcl"
						]
					},
					{
						"id": "fetchextended",
						"title": "Fetch your source code",
						"description": "Use the button below to open a list of debug configurations. Select your debug configuration from the list to fetch your source code. Enter your password if prompted. \n[Fetch Extended Sources](command:intertest-debug-dap.fetch-extended-source)",
						"media": {
							"image": "media/b5-fetch.png",
							"altText": "Fetch Extended Sources command in the F1 menu"
						},
						"completionEvents": [
							"onCommand:intertest-debug-dap.fetch-extended-source"
						]
					},
					{
						"id": "startsession",
						"title": "Start your debug session",
						"description": "Use the button below to start your debug session. Enter your password if prompted. \n[Run debug session](command:workbench.action.debug.start)",
						"media": {
							"image": "media/b6-start.png",
							"altText": "Start Debug Session button in the Run and Debug panel"
						},
						"completionEvents": [
							"onCommand:workbench.action.debug.start"
						]
					},
					{
						"id": "continue",
						"title": "Continue at START PROGRAM",
						"description": "The debug session stops at the START PROGRAM instruction. Use the Continue button to resume the session.",
						"media": {
							"image": "media/b7-startprogram.png",
							"altText": "Automatic breakpoint at the START PROGRAM instruction"
						}
					},
					{
						"id": "abend",
						"title": "Resolve abend at ADD 1 TO TASKNUM",
						"description": "The debug session stops at an abend on the instruction ADD 1 TO TASKNUM. This is because the variable TASKNUM has an invalid format. \nRight-click TASKNUM to set a new value, which must be a valid packed integer. You can also find TASKNUM under TASK-STRUCTURE in the variable tree.",
						"media": {
							"image": "media/b8-tasknum.png",
							"altText": "TASKNUM in the variables tree, under TASK-STRUCTURE"
						}
					},
					{
						"id": "breakpoint",
						"title": "Set an unconditional breakpoint at RETURN TO OPSYS",
						"description": "Click in the margin next to the line RETURN TO OPSYS to set an unconditional breakpoint at that line.",
						"media": {
							"image": "media/b9-breakpoint.png",
							"altText": "Unconditional breakpoint at RETURN TO OPSYS"
						}
					},
					{
						"id": "trace",
						"title": "Continue and view trace",
						"description": "Continue the debug session. It will stop at the breakpoint you just set. View the statement trace in the bottom left hand corner of the interface to see the path back to the abend.",
						"media": {
							"image": "media/b10-viewtrace.png",
							"altText": "Statement trace from RETURN TO OPSYS back to ADD 1 TO TASKNUM"
						}
					},
					{
						"id": "complete",
						"title": "Run the program to completion",
						"description": "Continue the debug session again to run it to completion. \nCongratulations on completing the Basic Batch Demo. Refer to the documentation to learn more batch debugging features available with Debugger for Mainframe. [Documentation (opens in new window)](https://marketplace.visualstudio.com/items?itemName=broadcomMFD.debugger-for-mainframe)",
						"media": {
							"image": "media/b11-continuecomplete.png",
							"altText": "Continue button in the Debug Toolbar"
						}
					}
				]
			},
			{
				"id": "sampleCICS",
				"title": "Basic CICS Demo Session",
				"description": "This walkthrough takes you step-by-step through a basic CICS demo debugging session in Debugger for Mainframe.",
				"steps": [
					{
