		"debuggers": [
			{
				"type": "intertest-cics",
				"label": "Debugger for Mainframe: INTERTEST™ FOR CICS",
				"runtime": "java",
				"runtimeArgs": [
					"-jar"
				],
				"program": "./bin/idas.jar",
				"configurationAttributes": {
					"launch": {
						"required": [
							"programName",
							"cicsApplId"
						],
						"anyOf": [
							{
								"required": [
									"host",
									"port",
									"interTestUserName"
								]
							},
							{
								"required": [
									"path",
									"apimlProfile"
								]
							}
						],
						"properties": {
							"cicsApplId": {
								"type": "string",
								"description": "CICS Application Id.",
								"minLength": 1,
								"maxLength": 8,
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in cicsApplId."
							},
							"cicsUserId": {
								"type": "string",
								"description": "Specifies a value if you want to debug a CICS program or transaction as it executes for a specific CICS user logon ID. You cannot modify this value during an active debugging session.",
								"default": ".ANY",
								"minLength": 1,
								"maxLength": 8,
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in cicsUserId."
							},
							"testMode": {
								"type": "boolean",
								"description": "Used only for internal testing.",
								"default": false
							},
							"programName": {
								"type": "array",
								"description": "CICS Program.",
								"minItems": 1,
								"maxItems": 30,
								"uniqueItems": true,
								"items": {
									"type": "string",
									"minLength": 1,
									"maxLength": 17,
									"pattern": "^[^_]\\S+$",
									"patternErrorMessage": "No spaces allowed in program name, program name cannot start with underscore."
								}
							},
							"host": {
								"type": "string",
								"description": "Intertest Server Host Name.",
								"minLength": 1,
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in host."
							},
							"port": {
								"type": "integer",
								"description": "Intertest Server Port Number.",
								"minimum": 0,
								"maximum": 65535
							},
							"interTestUserName": {
								"type": "string",
								"description": "Intertest Server User Name.",
								"minLength": "1",
								"maxLength": "8",
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in interTestUserName."
							},
							"interTestSecure": {
								"type": "boolean",
								"description": "Intertest Server Secure.",
								"default": false
							},
							"apimlProfile": {
								"type": "string",
								"description": "APIML Profile used for SSO"
							},
							"interTestCharset": {
								"type": "string",
								"description": "Intertest Server Charset for Listings.",
								"default": "UTF-8",
								"minLength": 1
							},
							"transactionId": {
								"type": "string",
								"description": "Name of CICS transaction",
								"minLength": 1,
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in transactionId."
							},
							"variableOrder": {
								"type": "string",
								"description": "Variables are ordered according to the position of their definition in the source code or alphabetically.",
								"enum": [
									"definition",
									"alphabetical"
								],
								"default": "definition"
							},
							"debugServer": {
								"type": "integer",
								"description": "DA port to connect (localhost).",
								"default": 4712
							},
							"paragraphBreakpoints": {
								"type": "boolean",
								"description": "Stop at paragraphs",
								"default": false
							},
							"logOutput": {
								"type": "string",
								"description": "Loggging destination preference",
								"enum": [
									"file",
									"console"
								]
							},
							"logLevel": {
								"type": "string",
								"description": "Loggging level preference",
								"enum": [
									"SEVERE",
									"WARNING",
									"INFO",
									"CONFIG",
									"FINE",
									"FINER",
									"FINEST",
									"ALL"
								]
							},
							"terminalId": {
								"type": "string",
								"description": "Allows you to specify a CICS terminal to the debug session. You cannot modify this value during an active debugging session.",
								"default": ".ANY",
								"minLength": 1
							},
							"callTrace": {
								"type": "boolean",
								"description": "Allows Calltrace data retrieval for the debug session, in order to populate Call Stack View.",
								"default": false
							},
							"statementTrace": {
								"type": "boolean",
								"description": "Enable fetching of statement trace.",
								"default": true
							},
							"path": {
								"type": "string",
								"description": "APIML path",
								"minLength": 1,
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in service ID."
							},
							"ZoweProfileName": {
								"type": "string",
								"description": "Zowe Profile name",
								"minLength": 1,
								"pattern": "^\\S+$",
								"patternErrorMessage": "No spaces allowed in Zowe Profile name."
							}
						}
					}
				},
				"initialConfigurations": [
					{
						"name": "Debugger for Mainframe: INTERTEST™ FOR CICS",
						"type": "intertest-cics",
						"request": "launch",
						"programName": [
